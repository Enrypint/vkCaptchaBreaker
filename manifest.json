{
  "name": "VKcomAutoCaptcha",
  "version": "1.5",
  "manifest_version": 3,
  "minimum_chrome_version": "88",
  "description": "Google Chrome Extension adapted for automatic captcha solving on vk.com",
  "homepage_url": "https://github.com/eremeyko/vkcomCaptchaBreaker",
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": ["*://vk.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content.js", 
        "models/captcha_model.onnx", 
        "models/ctc_model.onnx"],
      "matches": ["*://vk.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "48": "icon48_0.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48_1.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://vk.com/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    }
  ]
}