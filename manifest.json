{
  "manifest_version": 2,
  "name": "VKcomAutoCaptcha",
  "description": "Google Chrome Extension adapted for automatic captcha solving on vk.com",
  "author": "Defasium (https://github.com/Defasium) and Eremey (https://github.com/eremeyko)",
  "version": "1.5",
  "permissions": ["storage", "tabs"],
   "background": {
       "scripts": [
         "background.js"
       ],
       "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://vk.com/*"],
      "js": ["content.js"]
    }
  ],
 "icons": {"16": "icon16.png",
            "48": "icon48_1.png",
            "128": "icon128.png"},
  "browser_action": {
    "default_icon": "icon48_0.png"
  },
  "web_accessible_resources": ["models/*.onnx"]
}